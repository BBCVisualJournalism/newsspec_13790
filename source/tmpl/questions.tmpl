                        <img class="question__icon question1__icon" src="<%= pathStatic %>/common/img/test_medal.png" alt="<%= olympic_medal_icon_alt_text %> <%= question_1_number %>" />
                        <div class="question__number"><%= question_1_number %></div>
                        <p class="question__text"><%= question_1_question %></p>
                        <div class="answers">
                            <button class="button--answer" type="button" data-score="5">   <%= question_1_highest %>    </button>
                            <button class="button--answer" type="button" data-score="4">   <%= question_1_high %>       </button>
                            <button class="button--answer" type="button" data-score="3">   <%= question_1_mid %>        </button>
                            <button class="button--answer" type="button" data-score="2">   <%= question_1_low %>        </button>
                            <button class="button--answer" type="button" data-score="1">   <%= question_1_lowest %>     </button>
                        </div>
                    </div>
                </div>
                <div class="question__feedback question1__feedback hidden">
                    <div class="chevron">
                        <p class="feedback__text"><%= question_1_feedback %></p>
                    </div>
                </div>
